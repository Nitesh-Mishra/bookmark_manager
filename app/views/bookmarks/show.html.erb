<h1><%= @bookmark.title %></h1>
<p><%= @bookmark.link %></p>
<p>
  Tags:
  <% @bookmark.tags.each do |tag| %>
    <%= link_to tag.name, tag_path(tag) %>
  <% end %>
</p>

<%= link_to "Back to Bookmark List", bookmarks_path %> |
<%= link_to "Edit", edit_bookmark_path(@bookmark) %> |
<%= link_to "Delete", bookmark_path(@bookmark), method: :delete , data: {confirm: "Do you want to delete this bookmark?"}
 %>

